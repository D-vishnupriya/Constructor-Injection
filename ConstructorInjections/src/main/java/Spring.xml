<?xml version="1.0" encoding="UTF-8"?>

<beans  xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
    http://www.springframework.org/schema/beans/spring-beans-4.0.xsd"> 
    <!-- creat an object -->
    <bean id="empobj" class="com.edu.Employee"><!--  Employee eob=new Employee(1,"kiran") -->
      <constructor-arg value="1" type="int"></constructor-arg> 
      <constructor-arg value="Kiran" type="String"></constructor-arg>
    </bean>
    
    <bean id="empobj1" class="com.edu.Employee"><!--  Employee eob1=new Employee(2,"Manoj") -->
      <constructor-arg value="2" type="int"></constructor-arg> 
      <constructor-arg value="Manoj" type="String"></constructor-arg>
    </bean>
    </beans>
